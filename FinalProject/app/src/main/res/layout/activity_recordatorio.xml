<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".pagina.PaginaRecordatorio">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_recordatorio"
        android:layout_width="match_parent"
        android:layout_height="698dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.242" />

    <Button
        android:id="@+id/btn_home2"
        android:layout_width="98dp"
        android:layout_height="62dp"
        android:drawableTop="@drawable/house"
        android:text="Home"
        android:textColor="#FFFFFF"
        android:textSize="10sp"
        app:backgroundTint="#0085f2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.948" />

    <Button
        android:id="@+id/btn_perfil2"
        android:layout_width="98dp"
        android:layout_height="62dp"
        android:text="Perfil"
        android:textColor="#FFFFFF"
        app:backgroundTint="#0085f2"
        android:textSize="10sp"
        android:drawableTop="@drawable/user"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.948" />

    <Button
        android:id="@+id/btn_logro2"
        android:layout_width="98dp"
        android:layout_height="62dp"
        android:drawableTop="@drawable/trophy"
        android:text="Logros"
        android:textColor="#FFFFFF"
        android:textSize="10sp"
        app:backgroundTint="#0085f2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.667"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.948" />

    <Button
        android:id="@+id/btn_calendar2"
        android:layout_width="98dp"
        android:layout_height="62dp"
        android:drawableTop="@drawable/calendar"
        android:text="Calendario"
        android:textColor="#FFFFFF"
        android:textSize="10sp"
        app:backgroundTint="#0085f2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.329"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.948" />
</androidx.constraintlayout.widget.ConstraintLayout>